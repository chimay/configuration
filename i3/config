# {{{ Barre (workspaces, infos, tray, ...)

# Start i3bar to display a workspace bar (plus the system information i3status
# finds out, if available)

bar {

	i3bar_command /usr/bin/i3bar
	status_command i3status -c ~/racine/config/xwin/winman/i3/i3status.conf

	mode dock
	position bottom

	workspace_buttons yes

	font pango:DejaVu Sans Mono 12 Bold

	colors {
		background #000000
		statusline #5B3C11
		separator #666666

		focused_workspace  #5B3C11 #5B3C11 #000000
		active_workspace   #5B3C11 #5B3C11 #000000
		inactive_workspace #121212 #000000 #5B3C11
		urgent_workspace   #2f343a #5B3C11 #000000
	}

}

