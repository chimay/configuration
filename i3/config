# {{{ Autostart

#exec xfce4-panel

exec xbacklight -set 100

exec pkill fond-ecran ; ~/bin/zsh/graphix/fond-ecran.zsh 12 84 ~/graphix/list/wallpaper.gen >> ~/log/${HOST}/graphix/fond-ecran.log &

#exec pkill xscreensaver ; xscreensaver -nosplash &

exec setxkbmap fr && sleep 1
exec xmodmap ~/.Xmodmap >>! ~/log/${HOST}/keyboard/xmodmap.log 2>&1 &

exec pkill unclutter ; unclutter -display :0.0 -root -jitter 7 -idle 5 &

exec ~/bin/zsh/alarm/start-alarm-bat.zsh 15 10 5 3 2>&1 >>! ~/log/${HOST}/alarm/alerteBatterie.log &

exec pkill load_cycle_fix ; ~/bin/shell/dev/load_cycle_fix.sh &

exec pkill xclock ; xclock -digital -strftime "%H : %M  %A %d %B %Y" -update 60 &

exec pgrep mpd || { rm -f ~/music/mpd/pid ; mpd ~/racine/config/music/mpd.conf }

exec kteatime

# }}}

# {{{ Apparence

# Titre des fenêtres
