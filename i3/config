# {{{ Fenêtres

bindsym $mod+Tab       focus right
bindsym $mod+BackSpace focus left

# change focus
bindsym $mod+Left  focus left
bindsym $mod+Down  focus down
bindsym $mod+Up    focus up
bindsym $mod+Right focus right

# move focused window
bindsym $mod+Shift+Left  move left
bindsym $mod+Shift+Down  move down
bindsym $mod+Shift+Up    move up
bindsym $mod+Shift+Right move right

# kill focused window
bindsym $mod+Shift+X kill

# }}}

# {{{ Scission

# split in horizontal orientation
bindsym $mod+h split h

# split in vertical orientation
bindsym $mod+v split v

# }}}

# {{{ Modes

# enter fullscreen mode for the focused container
bindsym $mod+f fullscreen

# change container layout (stacked, tabbed, default)
bindsym $mod+d layout default
bindsym $mod+s layout stacking
bindsym $mod+t layout tabbed

# }}}

# {{{ Fenêtres flottantes

# toggle tiling / floating

bindsym $mod+Shift+o floating toggle

# change focus between tiling / floating windows

bindsym $mod+o focus mode_toggle

# Use Mouse+$mod to drag floating windows to their wanted position

floating_modifier $mod

# }}}

# {{{ Signets

bindsym $mod+m exec i3-input -F 'mark %s' -P 'Nom du signet de la fenetre : '
bindsym $mod+apostrophe exec i3-input -F '[con_mark="%s"] focus' -P 'Aller au signet : '

# }}}

# {{{ Workspaces

default_orientation auto

workspace_layout tabbed
workspace_auto_back_and_forth yes

#bindsym $mod+minus workspace prev
#bindsym $mod+plus  workspace next

# switch to workspace
bindsym $mod+KP_1 workspace 1 Texte
bindsym $mod+KP_2 workspace 2 Navigateurs
bindsym $mod+KP_3 workspace 3 Bureautique
bindsym $mod+KP_4 workspace 4 Litterature
bindsym $mod+KP_5 workspace 5 Musique
bindsym $mod+KP_6 workspace 6 Dessin
bindsym $mod+KP_7 workspace 7 Cinema
bindsym $mod+KP_8 workspace 8 Jeux
bindsym $mod+KP_9 workspace 9 Graphique
bindsym $mod+KP_0 workspace 10

# switch to workspace
bindsym Ctrl+KP_1 workspace 1 Texte
bindsym Ctrl+KP_2 workspace 2 Navigateurs
bindsym Ctrl+KP_3 workspace 3 Bureautique
bindsym Ctrl+KP_4 workspace 4 Litterature
bindsym Ctrl+KP_5 workspace 5 Musique
bindsym Ctrl+KP_6 workspace 6 Dessin
bindsym Ctrl+KP_7 workspace 7 Cinema
bindsym Ctrl+KP_8 workspace 8 Jeux
bindsym Ctrl+KP_9 workspace 9 Graphique
bindsym Ctrl+KP_0 workspace 10

# Move to workspace
bindsym $mod+Shift+KP_1 move workspace 1 Texte
bindsym $mod+Shift+KP_2 move workspace 2 Navigateurs
bindsym $mod+Shift+KP_3 move workspace 3 Bureautique
bindsym $mod+Shift+KP_4 move workspace 4 Litterature
bindsym $mod+Shift+KP_5 move workspace 5 Musique
bindsym $mod+Shift+KP_6 move workspace 6 Dessin
bindsym $mod+Shift+KP_7 move workspace 7 Cinema
bindsym $mod+Shift+KP_8 move workspace 8 Jeux
bindsym $mod+Shift+KP_9 move workspace 9 Graphique
bindsym $mod+Shift+KP_0 move workspace 10

# }}}

