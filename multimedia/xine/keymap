# start playback
Play {
	key = Return
	modifier = none
}

# playback pause toggle
Pause {
	key = space
	modifier = none
}

# stop playback
Stop {
	key = S
	modifier = none
}

# close stream
Close {
	key = C
	modifier = none
}

# take a snapshot
Snapshot {
	key = t
	modifier = none
}

# eject the current medium
