"imap <A-x> <toNormalMode>

" custom {{{1

" set all&
" mapclear
" comclear
" colorscheme default

"colorscheme golden-night
colorscheme paper

set restoretabs=all
set suspendonrestore=regular
set suspendtimeout=1800000
set suspendtimeoutignore=["ga//","gp//","g/saxo/"]
set timeoutlen=12000
set downloadpath=~/racine/gate/download
set notificationduration=3000
set commandhist=persistuseronly
set vimcommand=gvim-lite.sh

nnoremap <f1> <:tabnew><toCommandMode>help<Space>
nnoremap <s-f1> <:help>

nnoremap <c-n> <:notifications>
nnoremap <a-c> <:cookies>

nnoremap <f5> <:reloadconfig><cr><:echo 'configuration reloaded'>
nnoremap <f6> <:set adblocker=update>
nnoremap ZZ <:quit>

nnoremap o <:tabnew><toExploreMode>
"nnoremap o <toExploreMode><BS>
nnoremap O <toExploreMode><End>

cnoremap <tab> <nextSuggestion>
cnoremap <s-tab> <prevSuggestion>
cnoremap <up> <commandHistoryPrevious>
cnoremap <down> <commandHistoryNext>

nnoremap R <refreshTab>

nnoremap <bs> <backInHistory>
nnoremap <s-bs> <forwardInHistory>
nnoremap <c-h> <:history>

nnoremap <home> <scrollTop>
nnoremap <end> <scrollBottom>

nnoremap <s-pageup> <moveTabBackward>
nnoremap <s-pagedown> <moveTabForward>

nnoremap § <toCommandMode>buffer<Space>
nnoremap P <:pin>

nnoremap <a-q> <:quit>
nnoremap <c-up> <toTopSplitWindow>
nnoremap <c-left> <toLeftSplitWindow>
nnoremap <c-right> <toRightSplitWindow>
nnoremap <c-down> <toBottomSplitWindow>

nnoremap yy <pageToClipboard>
nnoremap p <:tabnew><openFromClipboard>
vnoremap <a-y> <p.copyText><toNormalMode>

nnoremap d <:write>
nnoremap <a-d> <:screenshot>

nnoremap <del> <:close>
nnoremap <insert> <reopenTab>

map <A-k0> <:buffer 0>
map <A-k1> <:buffer 1>
map <A-k2> <:buffer 2>
map <A-k3> <:buffer 3>
map <A-k4> <:buffer 4>
map <A-k5> <:buffer 5>
map <A-k6> <:buffer 6>
map <A-k7> <:buffer 7>
map <A-k8> <:buffer 8>
map <A-k9> <:buffer -1>
nmap <C-^> <:buffer #>

snoremap <up> <commandHistoryPrevious>
snoremap <down> <commandHistoryNext>
snoremap <a-e> <editWithVim>
snoremap <c-e> <editWithVim>

source ~/racine/config/webrowser/vieb/bookmarks
map <a-m> <pageToClipboard>:!echo<Space>set<Space>favoritepages+=<C-v><Space>>><Space>~/racine/config/webrowser/vieb/bookmarks<CR>
