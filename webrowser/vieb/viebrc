" ---- Mappings

" nnoremap wi <:devtools>
" nnoremap Sh <:history>
" nnoremap Ss <:help>
" nnoremap <C-A-p> <:print>
" nnoremap <C-W> <:quit>
" nnoremap ZQ <:quit>
" nnoremap gd <:write>
" nunmap [
" nunmap ]
" nunmap {
" nunmap }
" nnoremap [[ <previousPage>
" nnoremap ]] <nextPage>
" nnoremap {{ <previousPageNewTab>
" nnoremap }} <nextPageNewTab>
" nnoremap gU <openNewTabWithCurrentUrl><CR><toParentUrl>
" nnoremap g0 <:b 0>
" nnoremap g^ <:b -1>
" nnoremap g$ <:b -1>
" nnoremap <C-^> <:buffer #>
" nnoremap <C-Tab> <:buffer #>
" nnoremap ga <:tabnew>
" nnoremap gC <openNewTabWithCurrentUrl><CR>
" nnoremap gJ <moveTabForward>
" nnoremap gK <moveTabBackward>
" nnoremap <C-S-PageUp> <moveTabForward>
" nnoremap <C-S-PageDown> <moveTabBackward>
" " This is disabled as it prevents use of window split mappings.
" " See :h splitting for more information on window splits
" " nmap <C-w> <:close>
" nnoremap <del> <:close>
" nnoremap co <:lclose><:rclose>
" nnoremap = <zoomReset>
" nnoremap <C-p> <:pin>
" nnoremap <C-s> <stopLoadingPage>
