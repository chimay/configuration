# Exclusions {{{1

--exclude=tags
--exclude=TAGS

--exclude=Grenier

--exclude=cscope.*
--exclude=gtags.files

--exclude=*~
--exclude=.*.un~

--exclude=*.html
--exclude=*.php
--exclude=*.js

--exclude=*.epub

--exclude=*.elc

--exclude=*.elf
--exclude=*.o
--exclude=*.so
--exclude=*.pyc
--exclude=*.zwc

--exclude=*.dll
--exclude=*.obj
--exclude=*.bak
--exclude=*.exe

--exclude=*,v
--exclude=RCS
--exclude=CVS
--exclude=.git
--exclude=.hg
--exclude=.bzr
--exclude=*.mtn
--exclude=_darcs

--exclude=*.aux
--exclude=*.log
--exclude=*.out
--exclude=*.maf
